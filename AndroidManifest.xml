<?xml version="1.0" encoding="utf-8"?>
<manifest package="games.pack.protap" xmlns:android="http://schemas.android.com/apk/res/android"
    android:versionCode="11" android:versionName="3.1">
    <uses-sdk android:minSdkVersion="4"></uses-sdk>
    <application android:icon="@drawable/icon" android:label="@string/app_name"
        android:theme="@android:style/Theme.NoTitleBar">
        <activity android:name="MainMenuActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="HighScoreActivity"></activity>
        <activity android:name="ReactionActivity"></activity>
        <activity android:name="PracticeActivity"></activity>
        <activity android:name="BoxingActivity"></activity>
        <activity android:name="PracticeReactionActivity"></activity>
        <activity android:name=".PracticeBoxingActivity"></activity>
    </application>
    <uses-permission android:name="android.permission.INTERNET" />

</manifest>